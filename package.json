{
	"name": "nacha-viewer",
	"displayName": "NACHA Viewer",
	"description": "Displays NACHA file contents in human comprehensible structure",
	"version": "0.0.1",
	"publisher": "SureshMadadha",
	"license": "MIT",
	"author": {
		"name": "Suresh Madadha",
		"email": "msuresh007@hotmail.com",
		"url": "https://www.linkedin.com/in/suresh-madadha-6763821b9/"
	},
	"repository": {
		"type": "git",
		"url": "https://msuresh007@dev.azure.com/msuresh007/nacha-viewer/_git/nacha-viewer"
	  },	
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Formatters", "Visualization"
	],
	"keywords": ["nacha", "ach"],
	"activationEvents": [
		"onCustomEditor:nacha-viewer.achFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nacha-viewer.nachaViewer",
				"title": "Nacha Viewer"
			}
		],
		"customEditors": [
			{
				"viewType": "nacha-viewer.achFile",
				"displayName": "NACHA File Viewer",
				"selector": [
					{
						"filenamePattern": "*.ach"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
